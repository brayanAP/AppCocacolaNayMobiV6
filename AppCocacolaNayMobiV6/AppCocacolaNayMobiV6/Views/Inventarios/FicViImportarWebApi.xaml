<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppCocacolaNayMobiV6.Views.Inventarios.FicViImportarWebApi">

    <ContentPage.Content>
        <StackLayout Padding="10" Orientation="Horizontal">
            <StackLayout Padding="10">
                <Label Text="{Binding FicTextAreaImpInv}" HorizontalOptions="FillAndExpand" VerticalOptions="StartAndExpand"/>
                <Button Text="IMP. CATALOGO INV." VerticalOptions="EndAndExpand" Command="{Binding FicMecImportInv}"></Button>
            </StackLayout>
            <StackLayout Padding="10">
                <Label Text="{Binding FicTextAreaImpGen}" HorizontalOptions="FillAndExpand" VerticalOptions="StartAndExpand"/>
                <Button Text="IMP. CATALOGOS GEN." VerticalOptions="EndAndExpand"></Button>
            </StackLayout>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>